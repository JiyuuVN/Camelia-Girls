<pre class="brush:python">
# en
# Act 1 starts here.
init:
    # Declare language-specific characters used by this game.
    if persistent.lang == "english":
        $ classroom = "Class"
        $ teacher = "Teacher"
        $ unknown = "Unknown"
label act1_en:
    scene black
    play music music_netbook1
    $ renpy.pause(1.0)
    scene bg sunrise
    with introfade
    nvl clear
    nvl show dissolve
    play countermusic sound_typing
    net "<Дорогая Силлия...>"
    net "<Наконец-то я приехала в школу Камелии...>"
    net "<С сегодняшнего дня я буду жить в общежитии, как и хотели мои родители...>"
    nvl hide dissolve
    stop countermusic
    scene bg academy
    with fade
    k_happy "Ах! Знала бы ты, какой  здесь чудесный утренний воздух..."
    k_relieved "И как прекрасен восход..."
    k_genki "Просто не терпится узнать, что же интересного со мной приключится..."
    stop music
    $ renpy.play(sound_collide)
    with vpunch
    nar "кто-то в меня врезался..."
    nar "...и я упала"
    show miharu grief at Fling(-0.5, 0.35, 1.5, 1.5, 1.0, 730.0)
    u "Хья-я-я!!!"
    nar "это какая-то девочка..."
    play music music_miharu1
    menu:
        "Я в порядке. Но, может, она поранилась?":
            jump okay_en
        "Тупая растяпа, смотри куда идёшь!":
            jump eyes_en
        "Хе-хе, она выглядит растерянной. Надо подшутить над ней.":
            jump bone_en
label okay_en:
    k_worried "Ай... Ох! С тобой всё нормально?"
    show miharu whimper at Rise(0.5, 1.0)
    u "Извини. Да, со мной всё нормально. А с тобой?"
    k_relieved "Ага, со мной тоже."
    show miharu relieved
    u "Рада слышать."
    show miharu uncomfortable at Bounce(1)
    u "Ой! Чёрт! Я опаздываю!"
    show miharu smile
    u "Извини, мне надо бежать, поэтому..."
    show miharu genki at ExitLeft(0.75)
    u "УВИДИМСЯ!"
    k_nervous "Что?"
    jump who_en
label eyes_en:
    k_furious "Эй! Ты ослепла что ли?!"
    show miharu grief at Rise(0.5, 0.25)
    u "Хьяа-а-а!!! Извини!!!"
    show miharu uncomfortable at Bounce(1)
    u "Ах! Чёрт! Я опаздываю!"
    show miharu nervous
    u "Извини, но мне надо бежать... Поэтому..."
    show miharu grief at ExitLeft(0.5)
    u "Увииидимсяяя!!!"
    k_shout "Эй! Вернись!!!"
    k_stare "..."
    k_unamused "Ох... она уже убежала..."
    jump who_en
label bone_en:
    k_sneaky "ОЙЙЙ! МОЯ НОГА!!! ОНА СЛОМАНА!!! ОХ, Я УМИРАЮ... КАК БОЛЬНО!!!"
    show miharu grief at Rise(0.5, 0.5)
    u "Йа-а-а-а-а!!!  Что я наделала?!!"
    show miharu dread at Bounce(2)
    u "Надо вызвать скорую~! Надо вызвать скорую~! "
    k_genki "Эй... я просто шучу..."
    show miharu grief at Bounce(1)
    u "ОХ! Я ЖЕ ОПАЗДЫВАЮ!"
    show miharu confused at Bounce(2)
    u "Что же делать~? Что делать~?"
    k_weird "ЭЙ... ты меня вообще слышала?"
    show miharu whimper
    u "Да...*хнык*"
    k_smug "Извини... я просто пошутила..."
    show miharu grief at Bounce(4)
    u "НЕПРАВДА! ИЗ-ЗА МЕНЯ ТЫ СЛОМАЛА НОГУ!!!"
    k_ehhh "А?!"
    show miharu cautious at Bow(1)
    u "Обещаю, я возьму на себя всю ответственность! Но только после того, как вернусь с учёбы..."
    k_weird "Ррр... Хорошо..."
    show miharu uncomfortable
    u "Пожалуйста, найди кого-нибудь, кто сможет отвести тебя в больницу!.."
    show miharu whimper at ExitLeft(0.75)
    u "Увидимся!"
    k_gasp "Ох~!"
    k_unamused "..."
    k_sarcastic "Это что, типа, тактика «Хит-энд-Ран»?"
    jump who_en
label who_en:
    k_sigh "..."
    show miharu smile at RunInDistance(-0.5, 1.5, 0.6, 0.75)
    nar "она убежала"
    nar "кто это вообще такая?"
    stop music fadeout 3.5 
    $ mouse_visible = False
    hide bg academy
    with videofade_in
    scene white
    show miniOP_1
    show miniOP_2 behind miniOP_1
    show miniOP_3 behind miniOP_2
    $ renpy.pause(7.5)
    play music music_konobangumiwa1
    show konobangumiwa_1
    show konobangumiwa_2
    $ renpy.pause(10.25)
    $ mouse_visible = True
    scene black
    stop music
    $ renpy.pause(0.25)
    play sound sound_schoolchime
    show schoolchime
    show acttitle
    $ renpy.pause(7.5)
    scene bg classroom
    with dissolve
    play music music_aizawa1
    show aizawa glad at EnterLeft(0.0, 1.0)
    t "Итак, класс!"
    t "Как я уже говорила, сегодня к нам присоединится новый ученик."
    show aizawa grateful
    t "Ну что ж, можешь войти. Представься, пожалуйста!"
    show aizawa smile at ExitLeft(1.0)
    nar "я вошла... взгляды всего класса были прикованы ко мне, но я старалась не нервничать"
    k_happy "Привет... Меня зовут Кана."
    k_genki "Приятно познакомиться..."
    show crowd nervous behind aizawa at Rise(0.50, 0.50)
    c "{=whispered}Приятно познакомиться...{/=whispered}"
    show crowd nervous at Sit(0.75)
    nar "они произнесли это таким скучающим тоном..."
    show aizawa sigh at EnterLeft(0.0, 0.5)
    t "*Вздох*"
    show aizawa resign
    t "Какое неоригинальное приветствие..."
    show aizawa fatigue
    stop music 
    k_nervous "А?!"
    nar "я слышу, как ученики перешёптываются: «Ох... опять она за старое...»"
    show aizawa chastisecenter
    t "Где СТРАСТЬ, Хана?! Слишком мало СТРАСТИ!.."
    show aizawa sadistic
    t "ДАВАЙ СНОВА!!!"
    show aizawa revenge
    play music music_aizawa2
    k_omg "Эээ?!!"
    k_nervous "...."
    k_genki "Это типа подшучивание над новенькими такое?.."
    show aizawa reprimand at Bounce(2)
    t "Конечно же нет!"
    show aizawa maniac
    t "А ТЕПЕРЬ ДАВАЙ, ПРЕДСТАВЬСЯ НОРМАЛЬНО!!!"
    show aizawa astound at ExitLeft(0.25)
    k_swirl "КЬЯЯЯЯ~!!! ХОРОШО! ХОРОШО!"
    k_nervous "Кхм..."
    menu:
        "Надо просто подыграть, так будет безопаснее...":
            jump hana_en
        "Она хочет страсти? ОНА ПОЛУЧИТ ЕЁ!!!":
            jump sama_en
        "Ох! Просто скажу, что в голову взбредёт...":
            jump legend_en
label hana_en:
    k_genki "Меня зовут Кана, а не Хана."
    stop music 
    play sound sound_deadsilence
    c "{=whispered}*тишина*{/=whispered}"
    nar "я вроде нормально представилась..."
    show aizawa chortlecenter at EnterLeft(0.0, 0.75)
    t "Хорошо, ты прошла..."
    show aizawa schemecenter
    k_panic "И ЭТО ВСЁ?!!"
    jump intro_en
label sama_en:
    stop music
    k_creepy "{=whispered}хухуху...{/=whispered}"
    play music music_pompous1
    k_evil "Я — ВЕЛИКАЯ И МОГУЩЕСТВЕННАЯ КАНА-САМА!!!"
    k_villainous "ПАДИТЕ НИЦ, СМЕРТНЫЕ~ !!!"
    stop music
    play sound sound_deadsilence
    c "{=whispered}*тишина*{/=whispered}"
    nar "«Ох, бедняжка...» Я снова слышу шёпот одноклассников"
    nar "как неудобно..."
    play music music_aizawa2
    show aizawa obsessed at EnterLeft(0.0, 0.25)
    t "БРАВО!!!"
    show aizawa love
    t "КАКОЕ ВЕЛИКОЛЕПНОЕ ПРЕДСТАВЛЕНИЕ!!! Я ТАК РАДА, ЧТО ГОТОВА РАСПЛАКАТЬСЯ!!!"
    show aizawa tearsofpride
    k_ehhh "ЧТО?!!"
    show aizawa squeal
    t "Ты мне уже нравишься, сестра!"
    show aizawa savor
    k_waterfall "Что происходит?!"
    k_angry "И я не ваша сестра!"
    show aizawa sweetdream
    nar "но она уже перестала обращать на меня внимание"
    jump intro_en
label legend_en:
    k_squeal "Я - легенда!"
    stop music
    play sound sound_embarrass
    c "*гул*"
    nar "«Ох и зачем я сморозила такую глупость?»"
    show aizawa orly at EnterLeft(0.0, 1.0)
    t "О... Так тебе нравятся фильмы с этим актёром."
    show aizawa snicker
    k_panic "Это... не совсем так!.."
    show aizawa chortlecenter
    t "Авава, не надо смущаться из-за этого... Он всем девочкам нравится."
    show aizawa schemecenter
    show crowd angry behind aizawa at Rise(0.5, 0.25)
    c "ПЕРЕСТАНЬТЕ НЕСТИ ВСЯКУЮ ЧУШЬ!!!"
    show crowd angry at Sit(0.25)
    jump intro_en
label intro_en:
    show aizawa cute
    t "Отлично... Пришло время и мне представиться..."
    show aizawa smile
    stop music fadeout 1
    play sound sound_spotlight
    show ambient darken behind aizawa
    show prop spotlight at Appear(0.0, 0.0, 0.0, 0.0)
    nar "прожектор?!"
    play music sound_drumroll
    show aizawa awesome
    t "ВЕЛИКАЯ~!"
    show aizawa evilchuckle
    t "ИЗУМИТЕЛЬНАЯ~!"
    show aizawa love
    t "НЕСРАВНЕННАЯ~!"
    show aizawa impressed
    t "ВЫЗЫВАЮЩАЯ ТРЕПЕТ~!"
    show aizawa happy
    t "ПРЕКРАСНАЯ~!"
    show aizawa pleasantdream
    t "{=whispered}и самое главное...{/=whispered}"
    show aizawa grateful
    t "ЛЕГЕНДАРНАЯ~!"
    stop music
    play sound sound_drumtada
    show aizawa squeal at Bounce(1)
    t "АЙЗАВА-СЕНСЕЙ!!!"
    play music music_pompous2
    show aizawa smile at MoveTo(0.5, 1.0)
    hide ambient darken
    with dissolve
    hide prop spotlight
    with dissolve
    with Pause(1)
    show snowblossom
    with dissolve
    nar "она подошла и приняла драматическую позу"
    show aizawa genki
    ta "Приятно познакомиться!"
    show aizawa smile
    stop music fadeout 2
    hide snowblossom
    with dissolve
    k_weird "Эм... Ум... Ага... Мне тоже приятно... Айзава-сенсей..."
    queue music [ music_aizawa3, music_aizawa4, music_aizawa5 ]
    show aizawa lecture at MoveTo(0.0, 0.5)
    ta "ЯВИСЬ, СТАРОСТА!"
    show aizawa meditate
    show freya lecture behind aizawa at Rise(1.0, 0.25)
    f "Меня зовут Фрейя!"
    show aizawa complain
    show freya irritated
    ta "Тяжело, знаешь ли, запомнить имена тысячи учеников."
    show aizawa fatigue
    show freya shout at Bounce(3)
    f "Запомните хотя бы имена УЧЕНИКОВ СВОЕГО КЛАССА!"
    show aizawa chortleleft
    show freya scowl
    ta "Я не ваш классный руководитель и мне необязательно их помнить..."
    show aizawa schemeleft
    show freya annoyed
    show crowd shout behind freya at Rise(0.5, 0.25)
    c "ВЫ {b}НАШ{/b} КЛАССНЫЙ РУКОВОДИТЕЛЬ!!!"
    show crowd calm
    show aizawa confuzzled
    ta "Правда? Я думала мой класс 2-В..."
    show aizawa confused
    show crowd angry
    show freya bored
    c "ЭТО {b}И ЕСТЬ{/b} 2-В!!!"
    show crowd calm
    k_unamused "..."
    show aizawa resign
    nar "вдруг показалось, что свалить из школы — было бы не такой уж и плохой идеей..."
    show aizawa genki at Laugh(3)
    show freya pout
    ta "Ха-ха-ха! Не обращайте внимания на такие мелочи..."
    show aizawa smug
    show freya nag
    f "Что именно вы подразумеваете под «мелочами»?!"
    show aizawa awesome
    show freya upset
    ta "Проехали... Итак. Вы сделали домашнее задание?"
    show aizawa snide
    show crowd shout
    show freya angry
    c "Какое домашнее задание?!"
    show crowd calm
    show aizawa evilchuckle
    ta "Как какое? Конечно же, по современной истории."
    show aizawa smug
    show freya calm at Bounce(1)
    show freya annoyed
    f "Но вы учитель английского."
    show aizawa dumbfounded at Laugh(2)
    ta "А? Ой-ой! Я и забыла, ХАХА!"
    show aizawa nervous
    show crowd shout
    show freya tired
    c "ХВАТИТ УЖЕ, СЕНСЕЙ!"
    show crowd calm
    show aizawa sadistic
    show freya perturbed
    ta "Ох! Просто сдайте уже наконец домашку."
    show crowd angry
    show aizawa revenge
    show freya angry
    c "О КАКОМ ДОМАШНЕМ ЗАДАНИИ ВЫ ГОВОРИТЕ?!"
    play countermusic sound_chatter fadein 0.25
    show crowd angry at VChatter
    show aizawa reel at MoveTo(0.25, 0.25)
    show freya defensive at MoveTo(0.75, 0.25)
    $ renpy.pause(0.25)
    show aizawa scream at HChatter(0.25, 0.28)
    show freya shout at HChatter(0.75, 0.72)
    nar "все мои мечты о нормальном первом дне в школе начали потихоньку рассыпаться..."
    show crowd angry at Sit(0.25)
    show aizawa savor at ExitLeft(0.25)
    show freya bloodlust at ExitRight(0.25)
    $ renpy.pause(0.25)
    stop music fadeout 2
    stop countermusic fadeout 0.5
    scene bg cafeteria
    with fade
    "————— Обеденный перерыв —————"
    play music music_roughday fadein 2.0 
    k_sigh "*вздох*"
    nar "после этого представления я даже вилку поднять не в состоянии"
    show freya nervouschuckle at EnterLeft(0.0, 0.5)
    f "Мне тебя жалко."
    show freya nervous
    k_nervous "Почему?"
    show freya covet
    f "Айзава-сенсей..."
    show freya bashful
    menu:
        "Ну, согласись, она единственная в своём роде...":
            jump interesting_en
        "Похоже, она сбежала из психушки...":
            jump crazy_en
        "Может, она и странная, но у неё доброе сердце.":
            jump nice_en
label interesting_en:
    k_sneaky "Она... интересная..."
    show freya glad
    f "Ну... Можно сказать она... «уникальная»"
    show freya smile
    k_sarcastic "Уникальная... как редкое животное?"
    show freya cute at Bounce(1)
    f "Именно!"
    jump classified_en
label crazy_en:
    k_painful "Она сумасшедшая!"
    show freya nervouschuckle
    f "Она такая..."
    jump classified_en
label nice_en:
    k_genki "Думаю, она хороший человек."
    show freya omg at Laugh(2)
    f "А?! ТЕБЕ ЧТО, ПРОМЫЛИ МОЗГИ?!"
    show freya nervous
    k_oops "???"
    jump classified_en
label classified_en:
    show freya glad
    f "Кстати..."
    f "Можно тебя кое о чём спросить?"
    show freya smile
    k_happy "Конечно..."
    show freya sarcastic
    f "Почему ты сюда перевелась?"
    show freya uneasy
    stop music fadeout 1
    k_sad "?.."
    k_oops "А с этой школой что-то не так?"
    $ renpy.pause(1.0)
    show freya bashful
    $ renpy.pause(0.5)
    show freya tender
    $ renpy.pause(0.25)
    show freya love
    f "Нет. Мне просто интересно."
    show freya cute
    f "Все, кого я спрашивала, рассказывали разные интересные истории."
    show freya calm
    play music music_flashback1 fadein 2.0
    k_dream "Понятно... Ну..."
    show freya curious at ExitLeft(0.25)
    $ renpy.pause(0.25)
    scene bg flashback1
    with flashbackfade
    k_happy "Родители уехали работать за границу, а меня отправили сюда..."
    k_sad "Вернутся они нескоро, так что..."
    scene bg flashback2
    with flashbackfade
    k_happy "Других родственников у меня здесь нет..."
    k_genki "...поэтому они оставили меня на попечение директору этой школы, он давний друг нашей семьи."
    scene bg flashback3
    with flashbackfade
    k_tired "Больше особо и нечего рассказывать."
    k_happy "Но всё же..."
    k_dream "Уверена, я проведу незабываемое время в Камелии!"
    scene bg cafeteria
    with flashbackfade
    show freya sweetdream at EnterLeft(0.0, 0.5)
    f "*кивает*"
    k_relieved "А как насчёт тебя, Фрейя?"
    show freya pleasantdream
    f "Типа как у тебя..."
    show freya sweetdream
    stop music
    k_nervous "..."
    play music music_roughday
    k_unamused "Другими словами, совершенно ничего интересного."
    show freya mock
    f "Всякое бывает."
    show freya snicker
    k_sigh "*вздыхает*"
    show freya tender at ExitLeft(0.25)
    $ renpy.pause(0.25)
    stop music fadeout 2
    scene bg classroom
    with fade
    "————— После занятий —————"
    play music music_aizawa4 fadein 2.0
    show aizawa pleasantdream at EnterLeft(0.0, 0.25)
    ta "Что ж, на сегодня хватит."
    show aizawa glad
    ta "Если у вас есть какие-то вопросы, оставьте их на..."
    play sound sound_doorslam
    show aizawa astound
    nar "распахнулась дверь"
    show aizawa amused at MoveTo(1.0, 0.25)
    show miharu grief behind aizawa at EnterLeft(0.0, 0.25)
    u "ИЗВИНИТЕ, Я ОПОЗДАЛА!!!"
    k_shock "Э! ТЫ?!!"
    show aizawa naive
    show miharu confused
    u "Кто?.."
    u "Я?.."
    show miharu nervous
    show aizawa inquisitive
    k_sneaky "Мы «встречались» во дворе, помнишь?!"
    show miharu wonder
    show aizawa snide
    u "Ох..."
    show miharu curious
    show aizawa cool
    u "..."
    show miharu excited at Bounce(1)
    show aizawa evilsmile
    u "Ох!"
    queue music [ music_aizawa3, music_aizawa4, music_aizawa5 ]
    stop countermusic fadeout 3.0
    show aizawa evilchuckle at Bounce(3)
    show miharu smile
    ta "О, так вы уже знакомы?.."
    show aizawa evilsmile
    k_angry "НЕТ!"
    show aizawa grateful
    ta "А откуда это дружеское отношение?"
    show aizawa drunksmile
    k_stare "Эээ... Дружеское?"
    show miharu cute
    show aizawa awesome
    ta "Что ж... Наконец-то ты пришла, Михару, правда занятия уже закончились."
    show miharu sad at Bow(1)
    show aizawa snide
    m "Ох... Извините..."
    show aizawa genki at Laugh(3)
    ta "Ха-ха-ха! Не вопрос... Я такой замечательный человек, что прощаю тебя..."
    show crowd angry behind miharu at Rise(0.50, 0.25)
    show miharu happy
    show aizawa smile
    c "ЭТО ВЫ ЗАМЕЧАТЕЛЬНЫЙ ЧЕЛОВЕК?!! НУ-НУ..."
    show crowd angry at Sit(0.25)
    show miharu cute
    show aizawa happy
    ta "Можешь подойти в учительскую и взять материал за сегодняшний день."
    show miharu genki at Bow(1)
    show aizawa smile
    m "Большое спасибо! Вы лучшая, Айзава-сенсей!"
    show miharu smile at ExitLeft(0.25)
    show aizawa squeal at Laugh(3)
    ta "Ха-ха-ха!"
    show aizawa savor
    show crowd nervous behind aizawa at Rise(0.50, 0.25)
    c "Ты, наверное, шутишь..."
    show crowd nervous at Sit(0.25)
    show aizawa orly
    ta "Ну, теперь точно всё! В следующий раз не забывайте о домашнем задании!"
